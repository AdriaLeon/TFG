d_time":43320,"words":null,"text_size":22,"text_color":"#ffffffff","bg_color":"#00000000","alignment":0,"source_width":0.8785,"source_height":0.1651},{"text":"we want to do grown-up politics","start_time":43321,"end_time":45160,"words":null,"text_size":22,"text_color":"#ffffffff","bg_color":"#00000000","alignment":0,"source_width":0.7897,"source_height":0.1135},{"text":"which the electorate want us to do as well","start_time":45161,"end_time":47324,"words":null,"text_size":22,"text_color":"#ffffffff","bg_color":"#00000000","alignment":0,"source_width":0.836,"source_height":0.1135}]}